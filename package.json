{
  "name": "rag-lite",
  "version": "1.0.0",
  "description": "Lightweight customer-scoped RAG chatbot",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"tsx watch src/index.ts\" \"npx tailwindcss -i ./src/styles.css -o ./public/styles.css --watch\"",
    "dev:restart": "./start-server.sh",
    "build": "tsc && npx tailwindcss -i ./src/styles.css -o ./public/styles.css --minify",
    "start": "node dist/index.js",
    "seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "better-sqlite3": "^9.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "gpt-3-encoder": "^1.1.4",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.0",
    "pdf-parse": "^1.1.1",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.17",
    "@types/pdf-parse": "^1.1.4",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
